{
  "name": "bot-discord-2.0",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "description": "discordx bot",
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "bot",
    "discord",
    "typescript"
  ],
  "type": "module",
  "main": "build/client.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config --loader ts-node/esm/transpile-only --es-module-specifier-resolution=node src/client.ts",
    "dev": "nodemon -r dotenv/config --exec node --loader ts-node/esm/transpile-only --es-module-specifier-resolution=node src/client.ts",
    "serve": "node --es-module-specifier-resolution=node build/client.js"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.2",
    "@discordx/pagination": "^1.1.0",
    "@discordx/utilities": "^3.3.0",
    "discord.js": "^13.6.0",
    "discordx": "^9.1.12",
    "dotenv": "^14.2.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.0.0"
  }
}
